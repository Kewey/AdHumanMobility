{
  "kind": "collectionType",
  "collectionName": "disturbances",
  "info": {
    "singularName": "disturbance",
    "pluralName": "disturbances",
    "displayName": "Perturbation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "perturbations"
    },
    "instances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::instance.instance",
      "mappedBy": "perturbation"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Professionnel",
        "Particulier"
      ],
      "required": true
    },
    "car_type": {
      "type": "enumeration",
      "enum": [
        "Piéton",
        "Vélo",
        "Trottinette",
        "Voiture",
        "Camion"
      ],
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "En cours",
        "Terminé"
      ]
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 250
    },
    "location": {
      "type": "string",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true,
      "default": 0
    },
    "latitude": {
      "type": "float",
      "required": true,
      "default": 0
    },
    "relationship": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "referent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::referent.referent",
      "inversedBy": "perturbations"
    }
  }
}
