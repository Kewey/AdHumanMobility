{
  "kind": "collectionType",
  "collectionName": "disturbances",
  "info": {
    "singularName": "disturbance",
    "pluralName": "disturbances",
    "displayName": "Perturbation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "perturbations"
    },
    "evidences": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Professionnel",
        "Particulier"
      ],
      "required": true
    },
    "car_type": {
      "type": "enumeration",
      "enum": [
        "Piéton",
        "Vélo",
        "Trottinette",
        "Voiture",
        "Camion"
      ],
      "required": false
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "En cours",
        "Terminé"
      ]
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 250
    },
    "location": {
      "type": "string",
      "required": true
    },
    "longitude": {
      "type": "float",
      "required": true,
      "default": 0
    },
    "latitude": {
      "type": "float",
      "required": true,
      "default": 0
    },
    "relationship": {
      "type": "string"
    },
    "referent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::referent.referent",
      "inversedBy": "perturbations"
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "low",
        "medium",
        "high"
      ],
      "required": true
    },
    "disturbanceAt": {
      "type": "datetime",
      "required": true
    },
    "blurredEvidences": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "blurWaitingKey": {
      "type": "string",
      "private": true
    },
    "typologies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::typologie.typologie",
      "inversedBy": "perturbations"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "perturbations"
    },
    "sub_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sub-category.sub-category",
      "inversedBy": "perturbations"
    },
    "uuid": {
      "type": "customField",
      "required": false,
      "customField": "plugin::field-uuid.uuid"
    }
  }
}
